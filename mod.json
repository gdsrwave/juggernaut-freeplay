{
	"geode": "4.4.0",
	"gd": {
		"win": "2.2074"
	},
	"version": "v1.2.0",
	"id": "jcack.jfp",
	"name": "jfp",
	"developer": "jcack",
	"description": "test",
	"resources": {
		"spritesheets": {
			"jfp": [
				"resources/*.png"
			]
		}
	},
	"dependencies": {
		"hjfod.gmd-api": {
			"importance": "required",
			"version": ">=v1.4.0"
		},
		"geode.node-ids": {
			"importance": "required",
			"version": ">=v1.20.0"
		}
	},
	"settings": {
		"speed": {
			"name": "Initial Speed",
			"description": "Possible values (0.5, 1, 2, 3, 4)",
			"type": "float",
			"default": 3
		},
		"length": {
			"name": "Length",
			"description": "The length of the level generated in m (1 block = 1 m). In JFP, length is measured in meters and height in units.",
			"type": "int",
			"default": 400
		},
		"seed": {
			"name": "RNG Seed",
			"description": "Set seed for level generation (0 for random)",
			"type": "string",
			"default": ""
		},
		"filename": {
			"name": "File Name",
			"description": "The name of the .gmd file saved during generation. File location: %LOCALAPPDATA%/GeometryDash/",
			"type": "string",
			"default": "waveman"
		},
		"marks": {
			"name": "Meter Marks",
			"type": "bool",
			"default": true
		},
		"marker-interval": {
			"name": "Mark Interval (m)",
			"type": "int",
			"default": 100
		},
		"corners": {
			"name": "Corner Pieces",
			"type": "bool",
			"default": false
		},
		"zig-limit": {
			"name": "Zigzagging Limit",
			"type": "bool",
			"default": true
		},
		"remove-spam": {
			"name": "Spam Limit",
			"type": "bool",
			"default": true
		},
		"low-vis": {
			"name": "Low Visibility",
			"type": "bool",
			"default": false
		},
		"corridor-height": {
			"name": "Corridor Height (units)",
			"description":  "1 block = 30 units. In JFP, length is measured in meters and height in units.",
			"type": "float",
			"default": 60
		},
		"portals": {
			"name": "Portal Spawning",
			"type": "bool",
			"default": true
		},
		"debug": {
			"name": "Debug",
			"type": "bool",
			"default": true
		}
	}
}